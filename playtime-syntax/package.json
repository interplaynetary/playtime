{
  "name": "playtime-dsl",
  "displayName": "Playtime DSL",
  "private": true,
  "description": "It's playtime!",
  "publisher": "ruzfronx",
  "version": "0.0.3",
  "repository": {},
  "engines": {
    "vscode": "^1.85.2"
  },
  "license": "MIT",
  "categories": [
    "Programming Languages"
  ],
  "files": [
    "./syntaxes/playtime.tmLanguage.json"
  ],
  "contributes": {
    "languages": [
      {
        "id": "playtime-dsl",
        "aliases": [
