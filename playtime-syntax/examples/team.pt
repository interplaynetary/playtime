(context team-abc
  (roles
    (designer
      (requires 'has-taste)
      (scripts
        (design ('abc))
        (present ('bcd))))
    (manager
      (scripts
        (plan ('abc))
        (assess ('bcd)))
      (requires 'has-empathy 'is-confident))
    (engineer
      (scripts
        (code ('abc))
        (test ('bcd)))
      (requires 'is-pragmatic 'is-analytical')))
  (enactment
    (manager plan)
    (designer design)))

